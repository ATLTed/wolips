<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<fragment
   id="org.objectstyle.wolips.support"
   name="%WOLips.support.fragmentName"
   version="1.0.6"
   provider-name="%WOLips.providerName"
   plugin-id="org.objectstyle.wolips"
   plugin-version="1.0.6"
   match="compatible">

   <runtime>
      <library name="lib/wolips-support.jar">
         <packages prefixes="org.objectstyle.wolips.support"/>
      </library>
   </runtime>

   <requires>
      <import plugin="org.eclipse.jface.text"/>
   </requires>


<!-- =================================================================================== -->
<!-- Extension: View                                                                     -->
<!-- =================================================================================== -->
   <extension
         point="org.eclipse.ui.views">
      <view
            name="%WOLips.Support.View.Name"
            icon="icons/wolips/WOLips-icon.gif"
            category="org.objectstyle.wolips.ui"
            class="org.objectstyle.wolips.support.SupportView"
            id="org.objectstyle.wolips.support.SupportView">
      </view>
   </extension>
</fragment>
