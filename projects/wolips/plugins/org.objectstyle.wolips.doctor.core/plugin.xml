<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.objectstyle.wolips.doctor.core"
   name="WOLips Doctor Core Plug-in"
   version="1.0.6"
   provider-name="objectstyle.org"
   class="org.objectstyle.wolips.doctor.core.DoctorCorePlugin">

   <runtime>
      <library name="wolips-doctor-core.jar">
         <export name="*"/>
      </library>
   </runtime>

   <requires>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.objectstyle.wolips.commons"/>
   </requires>
   <extension-point id="issueProvider" name="Issue Provider" schema="schema/issueProvider.exsd"/>

</plugin>
