<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<fragment
   id="org.objectstyle.wolips.help"
   name="WOLips Help"
   version="1.0.6"
   provider-name="objectstyle.org"
   plugin-id="org.objectstyle.wolips"
   plugin-version="1.0.6"
   match="compatible">

   <runtime>
      <library name="lib/wolips-help.jar">
         <packages prefixes="org.objectstyle.wolips.help"/>
      </library>
   </runtime>


<!-- ============================================== -->
<!-- Define the main book                           -->
<!-- ============================================== -->
   <extension
         point="org.eclipse.help.toc">
      <toc
            file="toc_root.xml"
            primary="true">
      </toc>
   </extension>
</fragment>
