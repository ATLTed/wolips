<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.objectstyle.wolips.woproject.ant"
   name="WOProject Ant"
   version="1.0.6"
   provider-name="objectstyle.org">

   <runtime>
   </runtime>

<!-- =================================================================================== -->
<!-- Extension: Ant                                                                      -->
<!-- =================================================================================== -->
   <extension
         point="org.eclipse.ant.core.extraClasspathEntries">
      <extraClasspathEntry
            library="lib/woproject.jar">
      </extraClasspathEntry>
   </extension>
   <extension
         point="org.eclipse.ant.core.antTasks">
      <antTask
            library="lib/woproject.jar"
            name="woapplication"
            class="org.objectstyle.woproject.ant.WOApplication">
      </antTask>
      <antTask
            library="lib/woproject.jar"
            name="woframework"
            class="org.objectstyle.woproject.ant.WOFramework">
      </antTask>
      <antTask
            library="lib/woproject.jar"
            name="wogen"
            class="org.objectstyle.woproject.ant.WOGenerator">
      </antTask>
      <antTask
            library="lib/woproject.jar"
            name="wocompile"
            class="org.objectstyle.woproject.ant.WOCompile">
      </antTask>
      <antTask
            library="lib/woproject.jar"
            name="pbindex"
            class="org.objectstyle.woproject.ant.PBIndex">
      </antTask>
      <antTask
            library="lib/woproject.jar"
            name="pbxindex"
            class="org.objectstyle.woproject.ant.PBXIndex">
      </antTask>
      <antTask
            library="lib/woproject.jar"
            name="pbxindex2"
            class="org.objectstyle.woproject.ant.WOPBXBuild">
      </antTask>
   </extension>

</plugin>
