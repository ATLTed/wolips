<?xml version="1.0" encoding="UTF-8"?>

<document>
  <properties>
    <title>Maven WOApplication Plugin Properties</title>
  </properties>
  <body>
    <section name="Maven WOApplication Plugin Settings">
      <table>
        <tr>
          <th>Property</th>
          <th>Optional?</th>
          <th>Description</th>
        </tr>
        <tr>
          <td>maven.woapplication.resources.include.patternset</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>${maven.woapplication.patternsets.dir}/resources.include.patternset</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.woapplication.resources.dir</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>${maven.build.dest}/Resources</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.woapplication.info.plist</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>${maven.woapplication.resources.dest.dir}/Info.plist</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.woapplication.artifact.tar</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>${maven.build.dir}${file.separator}${maven.final.name}.tar</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.woapplication.info.plist.dest</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>${maven.woapplication.resources.dir}/Info.plist</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.woapplication.patternsets.dir</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>${maven.woapplication.basedir}/woproject</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.woapplication.java.dest.dir</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>${maven.build.dir}${file.separator}${maven.final.name}.woa${file.separator}Resources${file.separator}Java</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.woapplication.dir</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>${maven.build.dir}/${maven.final.name}.woa</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.woapplication.classes.dir</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>${maven.build.dest}</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.woapplication.artifact</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>${maven.woapplication.artifact.tar.gz}</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.woapplication.wsresources.exclude.patternset</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>${maven.woapplication.patternsets.dir}/wsresources.exclude.patternset</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.ordering.dir</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>${maven.build.dir}${file.separator}ordering</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.woapplication.resources.exclude.patternset</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>${maven.woapplication.patternsets.dir}/resources.exclude.patternset</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.woapplication.basedir</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>${basedir}</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.woapplication.artifact.tar.gz</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>${maven.woapplication.artifact.tar}.gz</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.woapplication.wsresources.include.patternset</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>${maven.woapplication.patternsets.dir}/wsresources.include.patternset</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.woapplication.webserverresources.dir</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>${maven.build.dest}/WebServerResources</code>.</p>
          </td>
        </tr>
      </table>
    </section>
  </body>
</document>