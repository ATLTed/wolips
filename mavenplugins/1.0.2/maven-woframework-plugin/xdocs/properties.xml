<?xml version="1.0" encoding="UTF-8"?>

<document>
  <properties>
    <title>Maven WOFramework Plugin Properties</title>
  </properties>
  <body>
    <section name="Maven WOFramework Plugin Settings">
      <table>
        <tr>
          <th>Property</th>
          <th>Optional?</th>
          <th>Description</th>
        </tr>
        <tr>
          <td>maven.woframework.info.plist.dest</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>${maven.woframework.resources.dir}/Info.plist</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.woframework.resources.include.patternset</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>${maven.woframework.patternsets.dir}/resources.include.patternset</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.woframework.resources.dest.dir</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>${maven.woframework.dir}/Resources</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.woframework.artifact.tar.gz</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>${maven.woframework.artifact.tar}.gz</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.woframework.info.plist</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>${maven.woframework.resources.dest.dir}/Info.plist</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.woframework.java.dest.dir</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>${maven.woframework.resources.dest.dir}/Java</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.woframework.wsresources.exclude.patternset</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>${maven.woframework.patternsets.dir}/wsresources.exclude.patternset</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.woframework.artifact</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>${maven.woframework.artifact.tar.gz}</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.woframework.webserverresources.dest.dir</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>${maven.woframework.dir}/WebServerResources</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.woframework.artifact.tar</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>${maven.build.dir}/${maven.final.name}.tar</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.woframework.patternsets.dir</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>${maven.woframework.basedir}/woproject</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.woframework.resources.exclude.patternset</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>${maven.woframework.patternsets.dir}/resources.exclude.patternset</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.woframework.dir</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>${maven.build.dir}/${pom.artifactId}.framework</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.woframework.wsresources.include.patternset</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>${maven.woframework.patternsets.dir}/wsresources.include.patternset</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.woframework.webserverresources.dir</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>${maven.woframework.basedir}/WebServerResources</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.woframework.basedir</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>${basedir}</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.woframework.resources.dir</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>${maven.woframework.basedir}/Resources</code>.</p>
          </td>
        </tr>
      </table>
    </section>
  </body>
</document>