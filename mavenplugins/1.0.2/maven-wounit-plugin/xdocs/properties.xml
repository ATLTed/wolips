<?xml version="1.0" encoding="UTF-8"?>

<document>
  <properties>
    <title>Maven WOUnit Plugin Properties</title>
  </properties>
  <body>
    <section name="Maven WOUnit Plugin Settings">
      <table>
        <tr>
          <th>Property</th>
          <th>Optional?</th>
          <th>Description</th>
        </tr>
        <tr>
          <td>maven.wounit.launchandping.parameter.testsystem</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>-DTestSystem=YES</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.wounit.launchscript.unix</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>${maven.build.dir}/${maven.final.name}.woa/${maven.final.name}</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.wounit.launchandping.parameter.displayexceptionpages.value</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>true</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.wounit.launch.dir</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>${maven.build.dir}/${maven.final.name}.woa/</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.wounit.launch.timeout</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>270000</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.wounit.launchandping.port</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>12346</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.wounit.launchandping.parameter.autoopeninbrowser.value</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>NO</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.wounit.launchandping.parameter.directconnect.value</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>YES</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.wounit.launchandping.parameter.port.value</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>${maven.wounit.launchandping.port}</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.wounit.launchandping.parameter.port.argument</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>-WOPort</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.wounit.launchscript.win</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>${maven.build.dir}/${maven.final.name}.woa/${maven.final.name}.cmd</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.wounit.launchandping.parameter.displayexceptionpages.argument</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>-WODisplayExceptionPages</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.wounit.launchandping.parameter.directconnect.argument</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>-WODirectConnectEnabled</code>.</p>
          </td>
        </tr>
        <tr>
          <td>maven.wounit.launchandping.parameter.autoopeninbrowser.argument</td>
          <td>Yes</td>
          <td>
            <p>Default value is
              <code>-WOAutoOpenInBrowser</code>.</p>
          </td>
        </tr>
      </table>
    </section>
  </body>
</document>