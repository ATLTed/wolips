<?xml version="1.0" encoding="UTF-8"?>

<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<parent>
		<artifactId>woproject-all-parent</artifactId>
		<groupId>org.objectstyle.woproject</groupId>
		<version>2.2-SNAPSHOT</version>
	</parent>

	<modelVersion>4.0.0</modelVersion>

	<groupId>org.objectstyle.woproject.maven2</groupId>
	<artifactId>maven2</artifactId>
	<version>2.2-SNAPSHOT</version>
	<packaging>pom</packaging>
	<name>Maven2 All</name>

	<description>
		Contains WOProject Maven2 plugins collection.
	</description>

	<modules>
		<module>maven-wobootstrap-plugin</module>
		<module>maven-japplication-plugin</module>
		<module>maven-archetype-woapplication</module>
		<module>maven-wolifecycle-plugin</module>
		<module>maven-woexamples-plugin</module>
	</modules>

	<inceptionYear>2001</inceptionYear>

	<dependencyManagement>
		<dependencies>
			<dependency>
				<groupId>org.objectstyle.woproject.ant</groupId>
				<artifactId>woenvironment</artifactId>
				<version>[${globalRangeVersion},)</version>
			</dependency>
			<dependency>
				<groupId>org.objectstyle.woproject.ant</groupId>
				<artifactId>woproject-ant-tasks</artifactId>
				<version>[${globalRangeVersion},)</version>
			</dependency>
			<dependency>
				<groupId>commons-io</groupId>
				<artifactId>commons-io</artifactId>
				<version>1.2</version>
			</dependency>
			<dependency>
				<groupId>org.easymock</groupId>
				<artifactId>easymock</artifactId>
				<version>2.2</version>
				<scope>test</scope>
			</dependency>
			<dependency>
				<groupId>commons-lang</groupId>
				<artifactId>commons-lang</artifactId>
				<version>2.1</version>
			</dependency>

			<!-- used for scripted plugins -->
			<dependency>
				<groupId>org.apache.maven</groupId>
				<artifactId>maven-script-ant</artifactId>
				<version>2.0.4</version>
			</dependency>

			<!-- used for scripted plugins -->
			<dependency>
				<groupId>org.apache.maven</groupId>
				<artifactId>maven-artifact-ant</artifactId>
				<version>2.0.4</version>
			</dependency>

			<!-- used for java plugins -->
			<dependency>
				<groupId>org.apache.maven</groupId>
				<artifactId>maven-plugin-api</artifactId>
				<version>2.0.4</version>
			</dependency>

			<!-- used as a Mojo superclass for tasks that need to infer dependencies  -->
			<dependency>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>dependency-maven-plugin</artifactId>
				<version>1.0</version>
			</dependency>

			<dependency>
				<groupId>org.apache.maven</groupId>
				<artifactId>maven-project</artifactId>
				<version>2.0.4</version>
			</dependency>

			<dependency>
				<groupId>org.codehaus.plexus</groupId>
				<artifactId>plexus-archiver</artifactId>
				<version>1.0-alpha-7</version>
			</dependency>

			<dependency>
				<groupId>org.apache.maven</groupId>
				<artifactId>maven-embedder</artifactId>
				<version>2.0.4</version>
			</dependency>
		</dependencies>
	</dependencyManagement>
</project>

