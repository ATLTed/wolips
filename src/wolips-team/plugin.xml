<?xml version="1.0" encoding="UTF-8"?>
<plugin
   id="org.objectstyle.wolips.team"
   name="%WOLips.team.pluginName"
   version="1.0.1"
   provider-name="%WOLips.providerName">

<!-- =================================================================================== -->
<!-- Required Plugins                                                                    -->
<!-- =================================================================================== -->   
   <requires>
   </requires>

<!-- =================================================================================== -->
<!-- Extension: Team file types                                                          -->
<!-- =================================================================================== -->
   <extension
         point="org.eclipse.team.core.fileTypes">
      <fileTypes
            type="text"
            extension="wod">
      </fileTypes>
      <fileTypes
            type="text"
            extension="woo">
      </fileTypes>
      <fileTypes
            type="text"
            extension="plist">
      </fileTypes>
      <fileTypes
            type="text"
            extension="eomodeld">
      </fileTypes>
      <fileTypes
            type="text"
            extension="api">
      </fileTypes>
      <fileTypes
            type="text"
            extension="postamble">
      </fileTypes>
      <fileTypes
            type="text"
            extension="preamble">
      </fileTypes>
      <fileTypes
            type="text"
            extension="d2wmodel">
      </fileTypes>
      <fileTypes
            type="text"
            extension="pbxuser">
      </fileTypes>
      <fileTypes
            type="text"
            extension="pbxproj">
      </fileTypes>
      <fileTypes
            type="text"
            extension="ini">
      </fileTypes>
      <fileTypes
            type="text"
            extension="mappings">
      </fileTypes>
   </extension>
<!-- =================================================================================== -->
<!-- Extension: Ignored team file types                                                          -->
<!-- =================================================================================== -->
   <extension
         point="org.eclipse.team.core.ignore">
      <ignore
            selected="true"
            pattern="*.woa">
      </ignore>
      <ignore
            selected="true"
            pattern="*.framework">
      </ignore>
      <ignore
            selected="true"
            pattern="*.eomodeld~">
      </ignore>
      <ignore
            selected="true"
            pattern=".DS_Store">
      </ignore>
      <ignore
            selected="true"
            pattern="ant.frameworks.wo.woroot">
      </ignore>
      <ignore
            selected="true"
            pattern="ant.frameworks.wo.wolocalroot">
      </ignore>
      <ignore
            selected="true"
            pattern="ant.frameworks.user.home">
      </ignore>
      <ignore
            selected="true"
            pattern="ant.classpaths.wo.woroot">
      </ignore>
   </extension>

</plugin>
