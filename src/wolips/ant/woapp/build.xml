<project name="${projectname}" default="export" basedir="${basedir}">

	<target name="build">
		<woapplication name="${applicationname}" destDir="${destdir}">
   			<classes dir="bin">
   			</classes>
   			<resources dir="Resources">
       			<include name="*.eomodeld/**"/>
       			<include name="*.wo/**"/>
       			<include name="*.api"/>
   			</resources>
   			<wsresources dir="WSResources">
       			<include name="Images/**"/>
   			</wsresources>
		</woapplication>
	</target>
	
</project>