<?xml version="1.0" encoding="UTF-8"?>
<plugin
   id="org.objectstyle.wolips"
   name="%WOLips.pluginName"
   version="0.9.8"
   provider-name="%WOLips.providerName"
   class="org.objectstyle.wolips.plugin.WOLipsPlugin">

   <runtime>
      <library name="lib/wolips.jar">
         <export name="*"/>
         <packages prefixes="org.objectstyle.wolips.actions,org.objectstyle.wolips.ant,org.objectstyle.wolips.builder,org.objectstyle.wolips.editor,org.objectstyle.wolips.ide,org.objectstyle.wolips.images,org.objectstyle.wolips.io,org.objectstyle.wolips.listener,org.objectstyle.wolips.logging,org.objectstyle.wolips.natures,org.objectstyle.wolips.plugin,org.objectstyle.wolips.preferences,org.objectstyle.wolips.project,org.objectstyle.wolips.resources,org.objectstyle.wolips.ui,org.objectstyle.wolips.utils,org.objectstyle.wolips.wizards,org.objectstyle.wolips.workbench"/>
      </library>
      <library name="lib/woproject.jar">
         <export name="*"/>
         <packages prefixes="org.objectstyle.woproject.ant,org.objectstyle.woproject.env,org.objectstyle.woproject.log4j,org.objectstyle.woproject.pb,org.objectstyle.woproject.util,org.objectstyle.woproject.wo"/>
      </library>
      <library name="lib/cayenne.jar">
         <export name="*"/>
         <packages prefixes="org.objectstyle.cayenne"/>
      </library>
      <library name="lib/commons-logging.jar">
         <export name="*"/>
         <packages prefixes="org.apache.commons.logging"/>
      </library>
   </runtime>
   <requires>
      <import plugin="org.apache.xerces"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.search"/>
      <import plugin="org.eclipse.ant.core"/>
      <import plugin="org.eclipse.ui.externaltools"/>
      <import plugin="org.eclipse.jdt.core"/>
      <import plugin="org.eclipse.jdt.ui"/>
      <import plugin="org.eclipse.jdt.debug.ui"/>
      <import plugin="org.eclipse.jdt.launching"/>
      <import plugin="org.eclipse.debug.core"/>
      <import plugin="org.eclipse.debug.ui"/>
      <import plugin="org.eclipse.ui.externaltools"/>
      <import plugin="org.apache.ant"/>
   </requires>


<!-- =================================================================================== -->
<!-- Extension: Project Wizards                                                          -->
<!-- =================================================================================== -->
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            name="%WOLips.webobjects.5.1.categoryName"
            id="org.objectstyle.wolips.wizards.webobjects.5.1">
      </category>
      <wizard
            name="%WOLips.Component"
            icon="icons/wobuilder/wocomponent_wiz.gif"
            category="org.objectstyle.wolips.wizards.webobjects.5.1"
            class="org.objectstyle.wolips.wizards.WOComponentCreationWizard"
            id="org.objectstyle.wolips.wizards.new.wo.5.1.component">
         <description>
         </description>
         <selection
               class="org.eclipse.core.resources.IResource">
         </selection>
      </wizard>
      <wizard
            name="%WOLips.Subproject"
            icon="icons/woproject/wosubproject_wiz.gif"
            category="org.objectstyle.wolips.wizards.webobjects.5.1"
            class="org.objectstyle.wolips.wizards.WOSubprojectCreationWizard"
            id="org.objectstyle.wolips.wizards.new.wo.5.1.subproject">
         <description>
         </description>
         <selection
               class="org.eclipse.core.resources.IResource">
         </selection>
      </wizard>
      <wizard
            name="%WOLips.Application"
            icon="icons/woproject/woapplication_wiz.gif"
            category="org.objectstyle.wolips.wizards.webobjects.5.1"
            class="org.objectstyle.wolips.wizards.WOApplicationCreationWizard"
            project="true"
            id="org.objectstyle.wolips.wizards.new.wo.5.1.application">
         <description>
         </description>
      </wizard>
      <wizard
            name="%WOLips.Framework"
            icon="icons/woproject/woframework_wiz.gif"
            category="org.objectstyle.wolips.wizards.webobjects.5.1"
            class="org.objectstyle.wolips.wizards.WOFrameworkCreationWizard"
            project="true"
            id="org.objectstyle.wolips.wizards.new.wo.5.1.framework">
         <description>
         </description>
      </wizard>
      <wizard
            name="%WOLips.EOModel"
            icon="icons/woproject/eomodel_wiz.gif"
            category="org.objectstyle.wolips.wizards.webobjects.5.1"
            class="org.objectstyle.wolips.wizards.EOModelCreationWizard"
            id="org.objectstyle.wolips.wizards.new.wo.5.1.eomodel">
         <description>
         </description>
         <selection
               class="org.eclipse.core.resources.IResource">
         </selection>
      </wizard>
   </extension>
<!-- =================================================================================== -->
<!-- Extension: Action set in the perspective                                            -->
<!-- =================================================================================== -->
   <extension
