<?xml version="1.0" encoding="UTF-8"?>
<plugin
   id="org.objectstyle.wolips"
   name="%WOLips.pluginName"
   version="0.9.2"
   provider-name="%WOLips.providerName"
   class="org.objectstyle.wolips.WOLipsPlugin">

   <runtime>
      <library name="lib/wolips.jar">
         <export name="*"/>
      </library>
      <library name="lib/woproject.jar">
         <export name="*"/>
      </library>
      <library name="lib/cayenne.jar">
         <export name="*"/>
      </library>
   </runtime>
   <requires>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.apache.ant"/>
      <import plugin="org.eclipse.ant.core"/>
      <import plugin="org.eclipse.jdt.core"/>
      <import plugin="org.eclipse.jdt.ui"/>
      <import plugin="org.apache.xerces"/>
      <import plugin="org.eclipse.jdt.core"/>
      <import plugin="org.eclipse.search"/>
      <import plugin="org.eclipse.debug.core"/>
      <import plugin="org.eclipse.debug.ui"/>
      <import plugin="org.eclipse.jdt.debug"/>
      <import plugin="org.eclipse.jdt.launching"/>
      <import plugin="org.eclipse.compare"/>
      <import plugin="org.apache.lucene"/>
      <import plugin="org.eclipse.ui.externaltools"/>
   </requires>

<!-- =================================================================================== -->
<!-- Extension: help                                                                     -->
<!-- =================================================================================== -->
   <extension
         point="org.eclipse.help.toc">
      <toc
            file="toc.xml"
            primary="true">
      </toc>
   </extension>
<!-- =================================================================================== -->
<!-- Extension: Project Wizards                                                          -->
<!-- =================================================================================== -->
  <extension
         point="org.eclipse.ui.newWizards">
      <category
            name="%WOLips.webobjects.5.1.categoryName"
            id="org.objectstyle.wolips.wizards.webobjects.5.1">
      </category>
      <wizard
            name="%WOLips.Component"
            icon="icons/wobuilder/wocomponent_wiz.gif"
            category="org.objectstyle.wolips.wizards.webobjects.5.1"
            class="org.objectstyle.wolips.wizards.WOComponentCreationWizard"
            id="org.objectstyle.wolips.wizards.new.wo.5.1.component">
         <description>
         </description>
         <selection
               class="org.eclipse.core.resources.IResource">
         </selection>
      </wizard>
      <wizard
            name="%WOLips.Subproject"
            icon="icons/woproject/wosubproject_wiz.gif"
            category="org.objectstyle.wolips.wizards.webobjects.5.1"
            class="org.objectstyle.wolips.wizards.WOSubprojectCreationWizard"
            id="org.objectstyle.wolips.wizards.new.wo.5.1.subproject">
         <description>
         </description>
         <selection
               class="org.eclipse.core.resources.IResource">
         </selection>
      </wizard>
      <wizard
            name="%WOLips.Application"
            icon="icons/woproject/woapplication_wiz.gif"
            category="org.objectstyle.wolips.wizards.webobjects.5.1"
            class="org.objectstyle.wolips.wizards.WOProjectCreationWizard"
            project="true"
            id="org.objectstyle.wolips.wizards.new.wo.5.1.application">
         <description>
         </description>
      </wizard>
      <wizard
            name="%WOLips.Framework"
            icon="icons/woproject/woframework_wiz.gif"
            category="org.objectstyle.wolips.wizards.webobjects.5.1"
            class="org.objectstyle.wolips.wizards.WOFrameworkCreationWizard"
            project="true"
            id="org.objectstyle.wolips.wizards.new.wo.5.1.framework">
         <description>
         </description>
      </wizard>
      <wizard
            name="%WOLips.EOModel"
            icon="icons/woproject/eomodel_wiz.gif"
            category="org.objectstyle.wolips.wizards.webobjects.5.1"
            class="org.objectstyle.wolips.wizards.EOModelCreationWizard"
            id="org.objectstyle.wolips.wizards.new.wo.5.1.eomodel">
         <description>
         </description>
         <selection
               class="org.eclipse.core.resources.IResource">
         </selection>
      </wizard>
   </extension>
<!-- =================================================================================== -->
<!-- Extension: Avtion set in the perspective                                            -->
<!-- =================================================================================== -->
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="%ElementCreationActionSet.label"
            description="%ElementCreationActionSet.description"
            visible="false"
            id="org.objectstyle.wolips.ElementCreationActionSet">
         <action
               label="%OpenWOComponentCreationWizard.label"
               icon="icons/wobuilder/wocomponent_wiz.gif"
               tooltip="%OpenWOComponentCreationWizard.tooltip"
               class="org.objectstyle.wolips.actions.OpenWOComponentCreationWizard"
               toolbarPath="Normal/WOLipsWizards"
               id="org.objectstyle.wolips.OpenWOComponentCreationWizard">
         </action>
         <action
               label="%OpenWOSubprojectCreationWizard.label"
               icon="icons/woproject/wosubproject_wiz.gif"
               tooltip="%OpenWOSubprojectCreationWizard.tooltip"
               class="org.objectstyle.wolips.actions.OpenWOSubprojectCreationWizard"
               toolbarPath="Normal/WOLipsWizards"
               id="org.objectstyle.wolips.OpenWOSubprojectCreationWizard">
         </action>
         <action
               label="%OpenWOProjectCreationWizard.label"
               icon="icons/woproject/woapplication_wiz.gif"
               tooltip="%OpenWOProjectCreationWizard.tooltip"
               class="org.objectstyle.wolips.actions.OpenWOProjectCreationWizard"
               toolbarPath="Normal/WOLipsWizards"
               id="org.objectstyle.wolips.OpenWOProjectCreationWizard">
         </action>
         <action
               label="%OpenWOFrameworkCreationWizard.label"
               icon="icons/woproject/woframework_wiz.gif"
               tooltip="%OpenWOFrameworkCreationWizard.tooltip"
               class="org.objectstyle.wolips.actions.OpenWOFrameworkCreationWizard"
               toolbarPath="Normal/WOLipsWizards"
               id="org.objectstyle.wolips.OpenWOFrameworkCreationWizard">
         </action>
         <action
               label="%OpenEOModelCreationWizard.label"
               icon="icons/woproject/eomodel_wiz.gif"
               tooltip="%OpenEOModelCreationWizard.tooltip"
               class="org.objectstyle.wolips.actions.OpenEOModelCreationWizard"
               toolbarPath="Normal/WOLipsWizards"
               id="org.objectstyle.wolips.OpenEOModelCreationWizard">
         </action>
         <action
               label="%EnableWOBuild.label"
               icon="icons/wobuild/enable_build.gif"
               tooltip="%EnableWOBuild.tooltip"
               class="org.objectstyle.wolips.actions.WOBuildEnableAction"
               toolbarPath="Normal/WOLipsWizards"
               id="WOBuild.Enable.ID">
         </action>
         <action
               label="%DisableWOBuild.label"
               icon="icons/wobuild/disable_build.gif"
               tooltip="%DisableWOBuild.tooltip"
               class="org.objectstyle.wolips.actions.WOBuildDisableAction"
               toolbarPath="Normal/WOLipsWizards"
               id="WOBuild.Disable.ID">
         </action>
      </actionSet>
   </extension>
<!-- =================================================================================== -->
<!-- Extension: Project actions                                                          -->
<!-- =================================================================================== -->
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            objectClass="org.eclipse.core.resources.IProject"
            adaptable="true">
         <filter
               name="nature"
               value="org.eclipse.jdt.core.javanature">
         </filter>
         <menu
               label="&amp;WOLips"
               path="additions"
               id="org.objectstyle.wolips">
            <separator
                  name="wobuilder">
            </separator>
         </menu>
         <action
               label="%WOBuilder.Remove"
               class="org.objectstyle.wolips.actions.WOBuilderAction"
               menubarPath="org.objectstyle.wolips/wobuilder"
               id="%WOBuilder.Remove.ID">
         </action>
         <action
               label="%WOApplicationBuilder.Set"
               class="org.objectstyle.wolips.actions.WOBuilderAction"
               menubarPath="org.objectstyle.wolips/wobuilder"
               id="%WOApplicationBuilder.Set.ID">
         </action>
         <action
               label="%WOFrameworkBuilder.Set"
               class="org.objectstyle.wolips.actions.WOBuilderAction"
               menubarPath="org.objectstyle.wolips/wobuilder"
               id="%WOFrameworkBuilder.Set.ID">
         </action>
      </objectContribution>
   </extension>
<!-- =================================================================================== -->
<!-- Extension: File actions                                                             -->
<!-- =================================================================================== -->
	<extension point="org.eclipse.ui.popupMenus">
		<objectContribution
			id="org.objectstyle.wolips.actions.OpenComponentAction"
			objectClass="org.eclipse.jdt.core.ICompilationUnit"
			adaptable="true"
			nameFilter="*.java">
			<action
				id="org.objectstyle.wolips.actions.OpenComponentAction"
				label="%WOLips.PopupMenu.OpenComponentAction"
				tooltip="%WOLips.PopupMenu.OpenComponentActionTip"
				menubarPath="additions"
				enablesFor="1"
				class="org.objectstyle.wolips.actions.OpenComponentAction">
			</action>
		</objectContribution>
		<objectContribution
			id="org.objectstyle.wolips.actions.OpenSourceAction"
			objectClass="org.eclipse.core.resources.IFile"
			adaptable="true"
			nameFilter="*.wod">
			<action
				id="org.objectstyle.wolips.actions.OpenSourceAction"
				label="%WOLips.PopupMenu.OpenSourceAction"
				tooltip="%WOLips.PopupMenu.OpenSourceActionTip"
				menubarPath="additions"
				enablesFor="1"
				class="org.objectstyle.wolips.actions.OpenSourceAction">
			</action>
		</objectContribution>
		<objectContribution
			id="org.objectstyle.wolips.actions.OpenSourceAction"
			objectClass="org.eclipse.core.resources.IContainer"
			adaptable="true"
			nameFilter="*.wo">
			<action
				id="org.objectstyle.wolips.actions.OpenSourceAction"
				label="%WOLips.PopupMenu.OpenSourceAction"
				tooltip="%WOLips.PopupMenu.OpenSourceActionTip"
				menubarPath="additions"
				enablesFor="1"
				class="org.objectstyle.wolips.actions.OpenSourceAction">
			</action>
		</objectContribution>				
	</extension>
<!-- =================================================================================== -->
<!-- Extension: Ant                                                                      -->
<!-- =================================================================================== -->
   <extension
         point="org.eclipse.ant.core.extraClasspathEntries">
      <extraClasspathEntry
            library="lib/woproject.jar">
      </extraClasspathEntry>
      <extraClasspathEntry
            library="lib/cayenne.jar">
      </extraClasspathEntry>
      <extraClasspathEntry
            library="lib/log4j-1.2beta4.jar">
      </extraClasspathEntry>
   </extension>
   <extension
         point="org.eclipse.ant.core.antTasks">
      <antTask
            library="lib/woproject.jar"
            name="woapplication"
            class="org.objectstyle.woproject.ant.WOApplication">
      </antTask>
      <antTask
            library="lib/woproject.jar"
            name="woframework"
            class="org.objectstyle.woproject.ant.WOFramework">
      </antTask>
      <antTask
            library="lib/woproject.jar"
            name="wogenerator"
            class="org.objectstyle.woproject.ant.WOGenerator">
      </antTask>
      <antTask
            library="lib/wolips.jar"
            name="updateframeworksets"
            class="org.objectstyle.wolips.ant.UpdateFrameworkIncludeFiles">
      </antTask>
      <antTask
            library="lib/wolips.jar"
            name="refreshview"
            class="org.objectstyle.wolips.ant.RefreshEclipse">
      </antTask>
      <antTask
            library="lib/wolips.jar"
            name="updateotherclasspaths"
            class="org.objectstyle.wolips.ant.UpdateOtherClasspathIncludeFiles">
      </antTask>
   </extension>
<!-- =================================================================================== -->
<!-- Extension: WOFramework Builder                                                      -->
<!-- =================================================================================== -->
   <extension
         id="woframeworkbuilder"
         name="%WOFrameworkBuilderName"
         point="org.eclipse.core.resources.builders">
      <builder>
         <run
               class="org.objectstyle.wolips.builder.WOFrameworkBuilder">
         </run>
      </builder>
   </extension>
<!-- =================================================================================== -->
<!-- Extension: WOApplication Builder                                                    -->
<!-- =================================================================================== -->
   <extension
         id="woapplicationbuilder"
         name="%WOApplicationBuilderName"
         point="org.eclipse.core.resources.builders">
      <builder>
         <run
               class="org.objectstyle.wolips.builder.WOApplicationBuilder">
         </run>
      </builder>
   </extension>
<!-- =================================================================================== -->
<!-- Extension: Editors                                                                  -->
<!-- =================================================================================== -->
   <extension
         id="org.objectstyle.wolips.editors"
         point="org.eclipse.ui.editors">
      <editor
            name="WOComponent Builder"
            default="true"
            icon="icons/wobuilder/webobjects_builder.gif"
            extensions="wod"
            launcher="org.objectstyle.wolips.editor.OpenSystemEditor"
            id="org.objectstyle.wolips.external.wod.editor">
      </editor>
      <editor
            name="EOModeler"
            default="true"
            icon="icons/eomodeler/webobjects_eomodeler.gif"
            extensions="eomodeld"
            launcher="org.objectstyle.wolips.editor.OpenSystemEditor"
            id="org.objectstyle.wolips.external.eomodeld.editor">
      </editor>
      <editor
            name="ProjectBuilderWO"
            default="false"
            icon="icons/projectbuilder/projectbuilderwo.gif"
            extensions="project"
            launcher="org.objectstyle.wolips.editor.OpenSystemEditor"
            id="org.objectstyle.wolips.external.eomodeld.projectbuilderwo">
      </editor>
   </extension>
<!-- =================================================================================== -->
<!-- Extension: Project Natures                                                          -->
<!-- =================================================================================== -->
   <extension
         id="applicationnature"
         name="org.eclipse.core.resources.natures"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="org.objectstyle.wolips.natures.WOApplicationNature">
         </run>
      </runtime>
      <requires-nature
            id="org.eclipse.jdt.core.javanature">
      </requires-nature>
   </extension>
   <extension
         id="frameworknature"
         name="org.eclipse.core.resources.natures"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="org.objectstyle.wolips.natures.WOFrameworkNature">
         </run>
      </runtime>
      <requires-nature
            id="org.eclipse.jdt.core.javanature">
      </requires-nature>
   </extension>
<!-- =================================================================================== -->
<!-- Extension: Perspective                                                              -->
<!-- =================================================================================== -->
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="WOLips"
            icon="icons/wolips/WOLips-icon.gif"
            class="org.objectstyle.wolips.ui.PerspectiveFactory"
            id="org.objectstyle.wolips.ui.Perspective">
      </perspective>
   </extension>
<!-- =================================================================================== -->
<!-- Extension: Startup                                                                  -->
<!-- =================================================================================== -->
   <extension
         point="org.eclipse.ui.startup">
   </extension>
<!-- =================================================================================== -->
<!-- Extension: Decorators                                                               -->
<!-- =================================================================================== -->
   <extension
         point="org.eclipse.ui.decorators">
      <decorator
            objectClass="org.eclipse.core.resources.IFolder"
            label="WebObjects"
            state="true"
            class="org.objectstyle.wolips.utils.WOLabelDecorator"
            id="org.objectstyle.wolips.utils.decorator">
      </decorator>
   </extension>
<!-- =================================================================================== -->
<!-- Extension: View                                                                     -->
<!-- =================================================================================== -->
   <extension
         point="org.eclipse.ui.views">
      <category
            name="%WOLipsCategoryName"
            id="org.eclipse.wolips.ui">
      </category>
      <view
            name="%WOLips.NavigatorViewName"
            icon="icons/wolips/WOLips-icon.gif"
            category="org.eclipse.wolips.ui"
            class="org.objectstyle.wolips.ui.Navigator"
            id="org.objectstyle.wolips.ui.Navigator">
      </view>
      <view
            name="%WOLips.WONavigatorViewName"
            icon="icons/wolips/WOLips-icon.gif"
            category="org.eclipse.wolips.ui"
            class="org.objectstyle.wolips.ui.WONavigator"
            id="org.objectstyle.wolips.ui.WONavigator">
      </view>
      <view
            name="%WOLips.ModelNavigatorViewName"
            icon="icons/wolips/WOLips-icon.gif"
            category="org.eclipse.wolips.ui"
            class="org.objectstyle.wolips.ui.ModelNavigator"
            id="org.objectstyle.wolips.ui.ModelNavigator">
      </view>
      <view
            name="%WOLips.ProductNavigatorViewName"
            icon="icons/wolips/WOLips-icon.gif"
            category="org.eclipse.wolips.ui"
            class="org.objectstyle.wolips.ui.ProductNavigator"
            id="org.objectstyle.wolips.ui.ProductNavigator">
      </view>
   </extension>
<!-- =================================================================================== -->
<!-- Extension: Preferences                                                              -->
<!-- =================================================================================== -->
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            name="%WOLips.preferences.viewName"
            category="%WOLipsCategoryName"
            class="org.objectstyle.wolips.preferences.PreferencesPage"
            id="org.objectstyle.wolips.preferences.PreferencesPage">
      </page>
   </extension>
<!-- =================================================================================== -->
<!-- Extension: Team file types                                                          -->
<!-- =================================================================================== -->    
	<extension point="org.eclipse.team.core.fileTypes">
	  <fileTypes extension="wod" type="text"/>
	  <fileTypes extension="woo" type="text"/>
	  <fileTypes extension="plist" type="text"/>
	  <fileTypes extension="eomodeld" type="text"/>
	  <fileTypes extension="api" type="text"/>
	  <fileTypes extension="postamble" type="text"/>
	  <fileTypes extension="preamble" type="text"/>
	  <fileTypes extension="d2wmodel" type="text"/>
	  <fileTypes extension="pbxuser" type="text"/>
	  <fileTypes extension="pbxproj" type="text"/>
	  <fileTypes extension="ini" type="text"/>
	  <fileTypes extension="mappings" type="text"/>
	</extension>
<!-- =================================================================================== -->
<!-- Extension: Resource filter                                                          -->
<!-- =================================================================================== -->    
	<extension point= "org.eclipse.ui.resourceFilters">
	<filter pattern = "*.woa" selected = "false" >
	</filter>
	<filter pattern = "*.framework" selected = "false" >
	</filter>
	<filter pattern = "*.java" selected = "false" >
	</filter>
	<filter pattern = "*.wod" selected = "false" >
	</filter>
	<filter pattern = "*.wod" selected = "false" >
	</filter>
	<filter pattern = "*.woo" selected = "false" >
	</filter>
	<filter pattern = "*.html" selected = "false" >
	</filter>
	<filter pattern = "*.api" selected = "false" >
	</filter>
	<filter pattern = "*.wo" selected = "false" >
	</filter>
	<filter pattern = "*.plist" selected = "false" >
	</filter>
	<filter pattern = "*.eomodeld" selected = "false" >
	</filter>
	<filter pattern = "*.d2wmodel" selected = "false" >
	</filter>
	<filter pattern = "*.pbxuser" selected = "false" >
	</filter>
	<filter pattern = "*.pbxuser" selected = "false" >
	</filter>
	<filter pattern = "PB.project" selected = "false" >
	</filter>
	<filter pattern = "*.postamble" selected = "false" >
	</filter>
	<filter pattern = "*.preamble" selected = "false" >
	</filter>
	<filter pattern = "makefile" selected = "false" >
	</filter>
	<filter pattern = "build.xml" selected = "false" >
	</filter>
</extension>
</plugin>
