<!-- =================================================================================== -->
<!-- Extension: Project actions                                                          -->
<!-- =================================================================================== -->
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            objectClass="org.eclipse.core.resources.IProject"
            adaptable="true">
         <filter
               name="nature"
               value="org.eclipse.jdt.core.javanature">
         </filter>
         <menu
               label="&amp;WOLips"
               path="additions"
               id="org.objectstyle.wolips">
            <separator
                  name="project">
            </separator>
            <separator
                  name="wobuilder">
            </separator>
            <separator
                  name="woframeworks">
            </separator>
         </menu>
         <action
               label="%UpdatePB.Project"
               class="org.objectstyle.wolips.actions.PBAction"
               menubarPath="org.objectstyle.wolips/project"
               id="%UpdatePB.Project.ID">
         </action>
         <action
               label="%WOBuilder.Remove"
               class="org.objectstyle.wolips.actions.WOBuilderAction"
               menubarPath="org.objectstyle.wolips/wobuilder"
               id="%WOBuilder.Remove.ID">
         </action>
         <action
               label="%WOApplicationBuilder.Set"
               class="org.objectstyle.wolips.actions.WOBuilderAction"
               menubarPath="org.objectstyle.wolips/wobuilder"
               id="%WOApplicationBuilder.Set.ID">
         </action>
         <action
               label="%WOFrameworkBuilder.Set"
               class="org.objectstyle.wolips.actions.WOBuilderAction"
               menubarPath="org.objectstyle.wolips/wobuilder"
               id="%WOFrameworkBuilder.Set.ID">
         </action>
         <action
               label="%WOSystemFramework.Add"
               class="org.objectstyle.wolips.actions.WOFrameworkAction"
               menubarPath="org.objectstyle.wolips/woframeworks"
               id="%WOSystemFramework.Add.ID">
         </action>
         <action
               label="%WOLocalFramework.Add"
               class="org.objectstyle.wolips.actions.WOFrameworkAction"
               menubarPath="org.objectstyle.wolips/woframeworks"
               id="%WOLocalFramework.Add.ID">
         </action>
      </objectContribution>
   </extension>
<!-- =================================================================================== -->
<!-- Extension: File actions                                                             -->
<!-- =================================================================================== -->
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            objectClass="org.eclipse.jdt.core.ICompilationUnit"
            adaptable="true"
            nameFilter="*.java"
            id="org.objectstyle.wolips.actions.OpenComponentAction">
         <action
               label="%WOLips.PopupMenu.OpenComponentAction"
               tooltip="%WOLips.PopupMenu.OpenComponentActionTip"
               class="org.objectstyle.wolips.actions.OpenComponentAction"
               menubarPath="additions"
               enablesFor="1"
               id="org.objectstyle.wolips.actions.OpenComponentAction">
         </action>
      </objectContribution>
      <objectContribution
            objectClass="org.eclipse.core.resources.IFile"
            adaptable="true"
            nameFilter="*.wod"
            id="org.objectstyle.wolips.actions.OpenSourceAction">
         <action
               label="%WOLips.PopupMenu.OpenSourceAction"
               tooltip="%WOLips.PopupMenu.OpenSourceActionTip"
               class="org.objectstyle.wolips.actions.OpenSourceAction"
               menubarPath="additions"
               enablesFor="1"
               id="org.objectstyle.wolips.actions.OpenSourceAction">
         </action>
      </objectContribution>
      <objectContribution
            objectClass="org.eclipse.core.resources.IContainer"
            adaptable="true"
            nameFilter="*.wo"
            id="org.objectstyle.wolips.actions.OpenSourceAction">
         <action
               label="%WOLips.PopupMenu.OpenSourceAction"
               tooltip="%WOLips.PopupMenu.OpenSourceActionTip"
               class="org.objectstyle.wolips.actions.OpenSourceAction"
               menubarPath="additions"
               enablesFor="1"
               id="org.objectstyle.wolips.actions.OpenSourceAction">
         </action>
         <action
               label="%WOLips.PopupMenu.DeleteComponent"
               tooltip="%WOLips.PopupMenu.DeleteComponentTip"
               class="org.objectstyle.wolips.actions.DeleteComponentAction"
               menubarPath="additions"
               enablesFor="1"
               id="org.objectstyle.wolips.actions.DeleteComponentAction">
         </action>
      </objectContribution>
      <objectContribution
            objectClass="org.eclipse.core.resources.IFile"
            adaptable="true"
            nameFilter="*.eomodeld"
            id="org.objectstyle.wolips.actions.EnableEOGeneration">
         <action
               label="%WOLips.PopupMenu.EnableEOGeneration.label"
               tooltip="%WOLips.PopupMenu.EnableEOGeneration.tooltip"
               class="org.objectstyle.wolips.actions.EnableEOGeneration"
               menubarPath="additions"
               enablesFor="1"
               id="org.objectstyle.wolips.actions.EnableEOGeneration">
         </action>
      </objectContribution>
      <objectContribution
            objectClass="org.eclipse.core.resources.IContainer"
            adaptable="true"
            nameFilter="*.eomodeld"
            id="org.objectstyle.wolips.actions.EnableEOGeneration">
         <action
               label="%WOLips.PopupMenu.EnableEOGeneration.label"
               tooltip="%WOLips.PopupMenu.EnableEOGeneration.tooltip"
               class="org.objectstyle.wolips.actions.EnableEOGeneration"
               menubarPath="additions"
               enablesFor="1"
               id="org.objectstyle.wolips.actions.EnableEOGeneration">
         </action>
      </objectContribution>
   </extension>
<!-- =================================================================================== -->
<!-- Extension: Ant                                                                      -->
<!-- =================================================================================== -->
   <extension
         point="org.eclipse.ant.core.extraClasspathEntries">
      <extraClasspathEntry
            library="lib/wolips.jar">
      </extraClasspathEntry>
      <extraClasspathEntry
            library="lib/woproject.jar">
      </extraClasspathEntry>
      <extraClasspathEntry
            library="lib/cayenne.jar">
      </extraClasspathEntry>
      <extraClasspathEntry
            library="lib/commons-logging.jar">
      </extraClasspathEntry>
   </extension>
   <extension
         point="org.eclipse.ant.core.antTasks">
      <antTask
            library="lib/woproject.jar"
            name="woapplication"
            class="org.objectstyle.woproject.ant.WOApplication">
      </antTask>
      <antTask
            library="lib/woproject.jar"
            name="woframework"
            class="org.objectstyle.woproject.ant.WOFramework">
      </antTask>
      <antTask
            library="lib/woproject.jar"
            name="wogen"
            class="org.objectstyle.woproject.ant.WOGenerator">
      </antTask>
      <antTask
            library="lib/wolips.jar"
            name="updateframeworksets"
            class="org.objectstyle.wolips.ant.UpdateFrameworkIncludeFiles">
      </antTask>
      <antTask
            library="lib/wolips.jar"
            name="refreshview"
            class="org.objectstyle.wolips.ant.RefreshEclipse">
      </antTask>
      <antTask
            library="lib/wolips.jar"
            name="updateotherclasspaths"
            class="org.objectstyle.wolips.ant.UpdateOtherClasspathIncludeFiles">
      </antTask>
   </extension>
