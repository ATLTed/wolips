<body bgcolor="$bodybg" text="$bodyfg" leftmargin="0" marginwidth="0" topmargin="0" marginheight="0">
 
#set ($allSections = $root.getChild("body").getChildren("section"))
#foreach ( $section in $allSections )
	#section ($section)
