<project name="yyyyy" default="export" basedir="..">

	<target name="init">
		<tstamp/>
		<property name="projectname"  value="yyyyy" />
		<property name="destdir" value="../../application-export" />
		<property name="applicationname"  value="yyyyy" />
	</target>

	<target name="build" depends="init">
    	<eclipse.incrementalBuild project="${projectname}" kind="incr"/>
	</target>

	<target name="export" depends="build">
		<woapplication name="${applicationname}" destDir="${destdir}">
   			<classes dir="bin">
   			</classes>
   			<resources dir="resources">
       			<include name="*.eomodeld/**"/>
   			</resources>
   			<wsresources dir="WSResources">
       			<include name="Images/**"/>
   			</wsresources>
		</woapplication>
	</target>
	
</project>