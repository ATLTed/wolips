<project name="yyyyy" default="build" basedir="..">

	<target name="init">
		<tstamp/>
		<property name="projectname"  value="yyyyy" />
		<property name="destdir" value="src" />
		<property name="modelname"  value="" />
	</target>


	<target name="generate" depends="init">
		<wogenerator
			model="${modelname}"
			destDir="${destdir}"
   			usepkgpath = "true"
   			template = "eotemplates/subclass.vm"
   			supertemplate = "eotemplates/superclass.vm">
		</wogenerator>
	</target>
	
	<target name="build" depends="generate">
    	<eclipse.incrementalBuild project="${projectname}" kind="incr"/>
	</target>

</project>