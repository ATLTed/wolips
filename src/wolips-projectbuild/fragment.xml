<?xml version="1.0" encoding="UTF-8"?>
<fragment
   id="org.objectstyle.wolips.projectbuild"
   name="%WOLips.projectbuild.fragmentName"
   version="1.0.0"
   provider-name="%WOLips.providerName"
   plugin-id="org.objectstyle.wolips"
   plugin-version="1.0.0"
   match="compatible">

   <runtime>
      <library name="lib/wolips-projectbuild.jar">
         <packages prefixes="org.objectstyle.wolips.projectbuild"/>
      </library>
   </runtime>


   <extension
         point="org.eclipse.jdt.core.classpathContainerInitializer">
      <classpathContainerInitializer
            class="org.objectstyle.wolips.projectbuild.classpath.WOClasspathContainerInitializer"
            id="org.objectstyle.wolips.WO_CLASSPATH">
      </classpathContainerInitializer>
   </extension>
   <extension
         point="org.eclipse.jdt.core.classpathContainerInitializer">
      <classpathContainerInitializer
            class="org.objectstyle.wolips.projectbuild.classpath.WOClasspathContainerInitializer"
            id="de.thecode.eclipse.wosupport.WO_CLASSPATH">
      </classpathContainerInitializer>
   </extension>
   <extension
         point="org.eclipse.jdt.ui.classpathContainerPage">
      <classpathContainerPage
            name="WebObjects Classpath"
            class="org.objectstyle.wolips.projectbuild.classpath.WOClassPathContainerPage"
            id="org.objectstyle.wolips.WO_CLASSPATH">
      </classpathContainerPage>
   </extension>
   <extension
         point="org.eclipse.jdt.ui.classpathContainerPage">
      <classpathContainerPage
            name="WebObjects Classpath"
            class="org.objectstyle.wolips.projectbuild.classpath.WOClassPathContainerPage"
            id="de.thecode.eclipse.wosupport.WO_CLASSPATH">
      </classpathContainerPage>
   </extension>
   <extension
         point="org.eclipse.ui.propertyPages">
      <page
            objectClass="org.eclipse.core.resources.IProject"
            adaptable="true"
            name="WOLips build"
            class="org.objectstyle.wolips.projectbuild.nature.WOLipsProjectNaturePage"
            id="org.objectstyle.wolips.projectbuild.nature.WOLipsProjectNaturePage">
         <filter
               name="nature"
               value="org.eclipse.jdt.core.javanature">
         </filter>
      </page>
   </extension>
   <extension
         id="incrementalnature"
         name="WO Lips incremental build Nature"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="org.objectstyle.wolips.projectbuild.nature.WOIncrementalBuildNature">
         </run>
      </runtime>
      <requires-nature
            id="org.eclipse.jdt.core.javanature">
      </requires-nature>
   </extension>
   <extension
         id="incrementalbuilder"
         name="WebObjects build-support"
         point="org.eclipse.core.resources.builders">
      <builder>
         <run
               class="org.objectstyle.wolips.projectbuild.WOProjectBuilder">
         </run>
      </builder>
   </extension>
</fragment>
